{
  "name": "hasura-om",
  "version": "0.0.6",
  "description": "Hasura object mapping",
  "main": "src/index.js",
  "scripts": {
    "test": "nyc  --reporter=html --reporter=lcovonly ava --verbose",
    "test_dev": "nyc --reporter=html --reporter=text ava --watch --verbose",
    "release": "npm-github-release",
    "coverage": "nyc report --reporter=text-lcov | coveralls"
  },
  "author": "Spartak <iam@spartak.io> (https://spartak.io/)",
  "license": "MIT",
  "ava": {
    "files": [
      "tests/**/*"
    ],
    "require": [
      "esm"
    ],
    "failWithoutAssertions": false,
    "concurrency": 16
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged --pattern \"**/*.*(js|jsx)\""
    }
  },
  "dependencies": {
    "axios": "^0.19.2"
  },
  "devDependencies": {
    "ava": "^3.8.2",
    "coveralls": "^3.1.0",
    "dotenv": "^8.2.0",
    "esm": "^3.2.25",
    "graphql": "^15.0.0",
    "graphql-tag": "^2.10.3",
    "husky": "^4.2.5",
    "nodemon": "^2.0.4",
    "nyc": "^15.0.1",
    "prettier": "^2.0.5",
    "pretty-quick": "^2.0.1"
  }
}
